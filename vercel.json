{
  "version": 2,
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "api/index.py" 
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "functions": {
    "api/index.py": {
      "memory": 1024,
      "maxDuration": 30,
      "excludeFiles": "**/__pycache__/**,**/tests/**,**/test/**,**/docs/**,**/*.pyc,**/numpy/doc/**,**/scipy/tests/**,**/pandas/tests/**,**/sklearn/datasets/data/**,**/sklearn/tests/**,**/shapely/tests/**"
    }
  }
}










